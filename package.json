{
  "name": "prisma-express-typescript-todo",
  "version": "1.0.0",
  "description": "A simple todo backend using Prisma, Express, TypeScript, and PostgreSQL",
  "main": "build/src/server.js",
  "scripts": {
    "compile": "tsc && node build/src/index.js",
    "dev": "DEBUG=app:* nodemon",
    "build": "tsoa spec-and-routes && tsc",
    "start": "babel-node -x .ts -- src/index.ts",
    "seed": "ts-node prisma/seed.ts",
    "test": "jest --coverage"
  },
  "keywords": [
    "prisma",
    "express",
    "typescript",
    "postgresql",
    "todo",
    "backend"
  ],
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "yarn start"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "babel-plugin-source-map-support": "^2.2.0",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@sinclair/typebox": "^0.26.8",
    "@types/debug": "^4.1.7",
    "axios": "^1.3.5",
    "babel-jest": "^29.5.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.7",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.3",
    "express-validator": "^6.15.0",
    "helmet": "^6.1.1",
    "jest": "^29.5.0",
    "joi": "^17.9.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "prisma": "^4.12.0",
    "prom-client": "^14.2.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "ts-jest": "^29.1.0",
    "zod": "^3.21.4"
  }
}
